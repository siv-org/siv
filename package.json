  "private": true,
  "scripts": {
    "build": "next build",
    "cypress": "cypress open --config-file cypress/cypress-config.json",
    "dev": "next dev",
    "e2e": "cypress run --config-file cypress/cypress-config.json",
    "format": "prettier --write **/*.{js,ts,tsx}",
